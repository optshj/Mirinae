        "@testing-library/react": "^16.3.0",
        "@types/auto-launch": "^5.0.5",
        "@types/node": "^22.17.0",
        "@types/react": "^19.1.1",
        "@types/react-dom": "^19.1.2",
        "@types/semver": "^7.7.1",
        "@vitejs/plugin-react": "^4.3.4",
        "electron": "^35.1.5",
        "electron-builder": "^25.1.8",
        "electron-vite": "^3.1.0",
        "globals": "^16.3.0",
        "jest": "^30.0.5",
        "jsdom": "^26.1.0",
        "node-gyp": "^11.4.2",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.13",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tailwindcss": "^4.1.11",
        "tw-animate-css": "^1.3.6",
        "typescript": "^5.8.3",
        "vite": "^6.2.6",
        "vitest": "^3.2.4",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
